    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="a, b, sisa, HP, HK" type="Integer" array="False" size=""/>
            <input variable="a"/>
            <input variable="b"/>
            <assign variable="sisa" expression="a mod 2"/>
            <if expression="sisa=0">
                <then>
                    <output expression="&quot;Bilangan pertama: Genap&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Bilangan pertama: Ganjil&quot;" newline="True"/>
                </else>
            </if>
            <assign variable="sisa" expression="b mod 2"/>
            <if expression="sisa=0">
                <then>
                    <output expression="&quot;Bilangan kedua: Genap&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Bilangan kedua: Ganjil&quot;" newline="True"/>
                </else>
            </if>
            <if expression="a=b">
                <then>
                    <output expression="&quot;Kedua bilangan sama besar&quot;" newline="True"/>
                </then>
                <else>
                    <if expression="a&gt;b">
                        <then>
                            <output expression="&quot;Bilangan pertama lebih besar&quot;" newline="True"/>
                        </then>
                        <else>
                            <output expression="&quot;Bilangan kedua lebih besar&quot;" newline="True"/>
                        </else>
                    </if>
                </else>
            </if>
            <if expression="a mod 2=0 and b mod 2=0">
                <then>
                    <assign variable="HP" expression="a+b"/>
                    <output expression="&quot;Hasil penjumlahan: &quot;&amp;HP" newline="True"/>
                </then>
                <else>
                    <if expression="a mod 2!=0 and b mod 2 !=0">
                        <then>
                            <assign variable="HK" expression="a*b"/>
                            <output expression="&quot;Hasil perkalian: &quot;&amp;HK" newline="True"/>
                        </then>
                        <else>
                            <if expression="a mod 2!=0 and b mod 2=0">
                                <then>
                                    <output expression="&quot;Berbeda jenis&quot;" newline="True"/>
                                </then>
                                <else>
                                    <if expression="a mod 2=0 and b mod 2!=0">
                                        <then>
                                            <output expression="&quot;Berbeda jenis&quot;" newline="True"/>
                                        </then>
                                        <else/>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
        </body>
    </function>
</flowgorithm>
